{
  "name": "usman-zahid/laravel-make-helper",
  "description": "A package to easily create static helper classes for your Laravel application",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Usman\\LaravelMakeHelper\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "authors": [
    {
      "name": "Usman Zahid",
      "email": "developerusman@yahoo.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "illuminate/console": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Usman\\LaravelMakeHelper\\MakeHelperServiceProvider"
      ]
    }
  }
}
